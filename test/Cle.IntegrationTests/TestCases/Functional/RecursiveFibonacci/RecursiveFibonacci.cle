// Computes the Fibonacci number F(10) using the horribly bad recursive method.
//
// Expected return code: 55.
// No console output expected.

namespace RecursiveFibonacci;

[EntryPoint]
public int32 Main()
{
    return Fib(10);
}

private int32 Fib(int32 n)
{
    if (n <= 2)
    {
        return 1;
    }
    else
    {
        return Fib(n-1) + Fib(n-2);
    }
}
