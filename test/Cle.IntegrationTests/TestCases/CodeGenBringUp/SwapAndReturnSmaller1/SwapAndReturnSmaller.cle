// Two Phi functions feeding each other in a loop.
//
// Expected return code: 10.
// No console output expected.

namespace SwapAndReturnSmaller;

[EntryPoint]
public int32 Main()
{
    // The loop is executed - the opposite is tested by SwapAndReturnSmaller2
    int32 a = 20;
    int32 b = 10;
    
    while (a > b)
    {
        int32 temp = a;
        a = b;
        b = temp;
    }

    return a;
}
